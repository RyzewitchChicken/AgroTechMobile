<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClientEdit">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/backopacity" />

    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="21dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="21dp"
        android:layout_marginBottom="31dp"
        android:background="@drawable/borderer"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="80dp"
            app:srcCompat="@drawable/login" />


        <TextView
            android:id="@+id/textView06"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            style="@style/edicontent"
            android:text="Nombre:" />

        <EditText
            android:id="@+id/namedit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="30dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:focusable="false"
            android:imeOptions="actionDone"
            android:singleLine="true"
            style="@style/edicontent"
            tools:ignore="Deprecated" />

        <TextView
            android:id="@+id/textView08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="60dp"
            android:text="DNI:"
            style="@style/edicontent"/>

        <EditText
            android:id="@+id/dniedit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="90dp"
            android:ems="10"
            android:inputType="number"
            android:focusable="false"
            style="@style/edicontent"/>


        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="120dp"

            android:text="INFORMACION CONTACTO "
            style="@style/datatitles"/>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="150dp"
            android:text="Correo: "
            style="@style/edicontent"/>

        <EditText
            android:id="@+id/emailedit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="180dp"
            android:ems="10"
            android:inputType="textPersonName"
            android:focusable="false"
            style="@style/edicontent"/>

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="210dp"
            android:text="ContraseÃ±a: "
            style="@style/edicontent"/>

        <EditText
            android:id="@+id/passedit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="240dp"
            android:ems="10"
            android:inputType="textPassword"
            android:focusable="false"
            style="@style/edicontent"/>

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="270dp"
            android:text="Telefono:"
            style="@style/edicontent"/>

        <EditText
            android:id="@+id/phoneedit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="300dp"
            android:ems="10"
            android:inputType="number"
            android:focusable="false"
            style="@style/edicontent"/>

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="20dp"
            android:text="Back"
            android:textColor="#FF008F39"
            android:textSize="15sp"
            android:lineSpacingMultiplier="0.27"/>

        <ImageView
            android:id="@+id/send"
            android:layout_width="39dp"
            android:layout_height="39dp"
            android:layout_gravity="center_horizontal"
            android:layout_marginLeft="110dp"
            android:layout_marginTop="250dp"
            android:layout_marginRight="20dp"
            app:srcCompat="@drawable/lapiz" />
        <ImageView
            android:id="@+id/imageedit"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="30dp"
            app:srcCompat="@drawable/lapiz" />
        <ImageView
            android:id="@+id/imagedni"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="90dp"
            app:srcCompat="@drawable/lapiz" />
        <ImageView
            android:id="@+id/imageemail"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="180dp"
            app:srcCompat="@drawable/lapiz" />
        <ImageView
            android:id="@+id/imagepass"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="240dp"
            app:srcCompat="@drawable/lapiz" />
        <ImageView
            android:id="@+id/imagephone"
            android:layout_width="17dp"
            android:layout_height="17dp"
            android:layout_gravity="center|left"
            android:layout_marginLeft="250dp"
            android:layout_marginTop="300dp"
            app:srcCompat="@drawable/lapiz" />

        <androidx.cardview.widget.CardView
            android:layout_width="300dp"
            android:layout_height="3dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="70dp"
            app:cardBackgroundColor="#008F39"/>

        <ImageView
            android:id="@+id/back"
            android:layout_width="12dp"
            android:layout_height="22dp"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:srcCompat="@drawable/raster" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="30dp"
            android:text="INFORMACION PERSONAL"
            style="@style/datatitles"/>

    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>